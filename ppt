from random import randint
x = randint (1,3)
# 1 pedra 
# 2 papel
# 3 testoura
escolha = int(input("Escolhe Pedra(1), Papel(2), Tesoura(3)"))
if x == escolha: 
    print("Empate")
if x == 1 and escolha == 2:
    print("Ganhaste")
if x == 1 and escolha == 3:
    print("Perdeste")
if x == 2 and escolha == ~1:
    print("Perdeste")
if x == 2 and escolha == 3:
    print("Ganhaste")
if x == 3 and escolha == 1:
    print("Ganhaste")
if x == 3 and escolha == 2:
    print("Perdeste")